{
  "name": "voidbox",
  "version": "1.2.0",
  "description": "Zero Background Image Generation Platform",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --watch src --watch server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "stop": "lsof -ti :3002 | xargs kill -9"
  },
  "keywords": [
    "image-generation",
    "ai",
    "background-removal"
  ],
  "author": "James Wolfe",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["./jest.setup.js"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
